{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "CarRequest",
    "type": "object",
    "properties": {
      "carId": {
        "type": "string",
        "description": "UUID for Car Loan"
      },
      "ownerId": {
        "type": "string",
        "description": "UUID for authenticated owner"
      },
      "createdAt": {
        "type": "string",
        "description": "Date car loan was created (JSON format)"
      },
      "make": {
        "type": "string",
        "description": "Make of car (e.g. Audi)"
      },
      "model": {
        "type": "string",
        "description": "Model of car (e.g. TT)"
      },
      "pictureUrl": {
        "type": "string",
        "description": "Signed URL for accessing picture of car"
      }
    },
    "required": ["make","model"],
    "additionalProperties": false
  }
